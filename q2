#include <iostream>
#include <string>
using namespace std;
string vol(string x){
  int i= x.length() - 1;
  string a= "";
  for (i;i>=0;i--){
    a = a + x[i];
  }

  return a;
}

int main(){
  string c;
  string a;
  cout <<"Dame una palabra para saber si es un palindromo" << endl;
  cin >> c;
  vol(c);
  if (a == c){
    cout <<"Si es un palindromo" << endl;
  }
  else {
    cout <<"No es un palindromo" << endl;
  }
  return 0;
}
